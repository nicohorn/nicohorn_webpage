generator client {
  provider = "prisma-client-js"
  previewFeatures = ["multiSchema"]
}
datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  schemas  = ["next_auth"]
}

model users {
  id         String      @id @default(uuid())
  name       String
  email      String      @unique
  image      String?
  role       String
  last_login DateTime?

  @@schema("next_auth")
  
}